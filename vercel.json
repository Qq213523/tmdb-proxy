{
  "version": 2,
  "rewrites": [
    {
      "source": "/movie/:path*",
      "destination": "/api/tmdb.js"
    },
    {
      "source": "/tv/:path*",
      "destination": "/api/tmdb.js"
    },
    {
      "source": "/person/:path*",
      "destination": "/api/tmdb.js"
    },
    {
      "source": "/search/:path*",
      "destination": "/api/tmdb.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/imaliang/tmdb-proxy",
      "permanent": true
    }
  ]
}
